- if @cdr.errors.any?
	= render 'layouts/errors', object: @cdr
= form_for @cdr, :remote=> true, :html => { :class => 'form-inline' } do |f|
	= f.hidden_field :atendimento_id
	= f.label :pontos, :class=>'control-label'
	= f.text_field :pontos, :class=>'input-mini'
	
	= f.submit class: "btn btn-primary"
