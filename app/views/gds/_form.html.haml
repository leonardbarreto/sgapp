- if @gd.errors.any?
	= render 'layouts/errors', object: @gd
.container-fluid
	= form_for @gd, :remote=> true do |f|
		= f.hidden_field :atendimento_id
		.row-fluid		
			.span9
				= f.label :pontos, :class=>'control-label'
				= f.text_field :pontos, :class=>'input-mini'
			.span3
				%label.control-label &nbsp;
				= f.submit class: "btn btn-primary"
				-#= button_tag(:type=>'submit',:class=>'btn btn-primary') do
					-#%i.icon-ok.icon-white
					-#Salvar
