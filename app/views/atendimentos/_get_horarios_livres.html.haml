.well.well-small
	%table.table.table-condensed
		%thead
			%th #
			%th Hor√°rio
			%th
			%th
			%th
		%tbody
			-if params[:getHorario]
				-@horarios.each_with_index do |h,index|
					%tr
						%td
							= index+1
						%td.horario_ini
							= @horarios.nil? ? "--": timeFormat(h.horario) 
						%td
							\-
