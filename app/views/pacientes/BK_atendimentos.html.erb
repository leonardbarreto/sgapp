<table class="table table-hover">
	<thead>
	<tr>
		<th>#</th>
		<th>Tipo de atendimento</th>	
		<th>Status</th>
		<th>Atendido em</th>
		
		
		<th>Funcion√°rio(a)</th>

	</tr>
	</thead>
	<tbody>

	<% @paciente.atendimento.order(:data_at).each_with_index do |at,index|  %>
		<% if at.status_id==4 %>
			<tr>
				<td><%= index+1 %></td>
				<td><%= at.tipo_atendimento.descricao %></td>
				<td><%= at.status.tipo %></td>
				<td><% if at.atendido? %>
						<%= dateFormat(at.data_at) %>
					<% else %>
						--
						<%#= at.status.tipo %>
					<% end %>

				</td>
				<td><%= at.funcionario.pessoa.nome %></td>
			</tr>
		<% end %>
	<% end %>
	</tbody>
</table>
		


